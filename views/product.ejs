  <script>
    document.querySelectorAll('.btn-cart').forEach(button => {
      button.addEventListener('click', async (event) => {
        try {
          const itemNameElements = document.getElementsByClassName('card-title');
          const itemImageElements = document.getElementsByClassName('img');
          const itemDescriptionElements = document.getElementsByClassName('card-text');
          const itemPriceElements = document.getElementsByClassName('card-price');
    
          // Declare variables outside the if block
          let itemName, itemImage, itemDescription, itemPrice;
    
          // Assuming you want to get the data from the first element in each collection
          if (itemNameElements.length > 0 && itemImageElements.length > 0 && itemDescriptionElements.length > 0 && itemPriceElements.length > 0) {
            itemName = itemNameElements[0].innerText.trim();
            itemImage = itemImageElements[0].src;
            itemDescription = itemDescriptionElements[0].innerText.trim();
            itemPrice = itemPriceElements[0].innerText.replace('$', '').trim();
          } else {
            alert('One or more elements not found');
            return;
          }
    
          const response = await fetch('/add-to-cart', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({
              itemName,
              itemImage,
              itemDescription,
              itemPrice
            })
          });
    
          if (response.ok) {
            const data = await response.json(); 
            if (data.alreadyInCart) {
              alert('Item already in the cart');
            } else {
              alert(`Item added to cart successfully! Item ID: ${data.itemId}`);
            }
          } else {
            const errorData = await response.json();
            alert(`Error adding item to cart: ${errorData.message}`);
          }
        } catch (error) {
          console.error('Error:', error);
        }
      });
    });
    </script>    
  
  <script>
    document.querySelectorAll('.btn-wish').forEach(button => {
      button.addEventListener('click', async (event) => {
        try {
          // Find the parent product card
          const itemNameElements = document.getElementsByClassName('card-title');
          const itemImageElements = document.getElementsByClassName('img');
          const itemDescriptionElements = document.getElementsByClassName('card-text');
          const itemPriceElements = document.getElementsByClassName('card-price')

          // Declare variables outside the if block
          let itemName, itemImage, itemDescription, itemPrice, currentDate;
    
          // Assuming you want to get the data from the first element in each collection
          if (itemNameElements.length > 0 && itemImageElements.length > 0 && itemDescriptionElements.length > 0 && itemPriceElements.length > 0) {
            itemName = itemNameElements[0].innerText.trim();
            itemImage = itemImageElements[0].src;
            itemDescription = itemDescriptionElements[0].innerText.trim();
            itemPrice = itemPriceElements[0].innerText.replace('$', '').trim();
            currentDate = new Date().toDateString();

          } else {
            alert('One or more elements not found');
            return;
          }
  
          const response = await fetch('/wish', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({
              itemName,
              itemImage,
              itemDescription,
              itemPrice,
              currentDate
            })
          });
  
          if (response.ok) {
            window.location.href = '/wish';
          } else {
            console.log('Item not displayed!');
          }
        } catch (error) {
          console.error('Error:', error);
        }
      });
    });
  </script>

 <script>
  document.querySelectorAll(".card-btn").forEach((button) => {
     button.addEventListener("click", (event) => {
    const productCard = event.target.closest(".card-recommended");
    if (!productCard) return;

    const itemName = productCard.querySelector(".card-title").innerText.trim();
    const itemImage = productCard.querySelector(".img").src;
    const itemDescription = productCard.querySelector(".card-text").innerText.trim();
    const itemPrice = productCard.querySelector(".card-price").innerText.replace("$", "").replace("View product", "").trim();

    // Build URL with query params
    const url = new URL("/Product", window.location.origin);
    url.searchParams.set("name", encodeURIComponent(itemName));
    url.searchParams.set("img", encodeURIComponent(itemImage));
    url.searchParams.set("desc", encodeURIComponent(itemDescription));
    url.searchParams.set("price", encodeURIComponent(itemPrice));

    // Redirect
    window.location.href = url.toString();
  });
});
</script>

