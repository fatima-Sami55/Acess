 <script>
  document.addEventListener("DOMContentLoaded", () => {
    const reviews = document.querySelectorAll(".review-item");
    const seeAllBtn = document.getElementById("seeAllBtn");
    const seeLessBtn = document.getElementById("seeLessBtn");

    let visibleCount = 2;
    const showIncrement = 2;

    // Show reviews logic
    seeAllBtn.addEventListener("click", (e) => {
      e.preventDefault();

      let shown = 0;
      for (let i = visibleCount; i < reviews.length && shown < showIncrement; i++) {
        reviews[i].classList.remove("d-none");
        shown++;
        visibleCount++;
      }

      if (visibleCount >= reviews.length) {
        seeAllBtn.classList.add("d-none");
      }

      if (visibleCount > 2) {
        seeLessBtn.classList.remove("d-none");
      }
    });

    // Hide reviews logic
    seeLessBtn.addEventListener("click", (e) => {
      e.preventDefault();

      for (let i = 2; i < reviews.length; i++) {
        reviews[i].classList.add("d-none");
      }

      visibleCount = 2;
      seeAllBtn.classList.remove("d-none");
      seeLessBtn.classList.add("d-none");
    });
  });
</script>

