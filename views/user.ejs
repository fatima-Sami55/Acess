<!-- Email verification flash -->
   <% if (!isVerified) { %>
      <div class="fixed-wrapper">
  <div class="container-fluid" style="padding: 0px;">
    <div class="alert alert-warning d-flex flex-column flex-md-row justify-content-between align-items-start align-items-md-center gap-2" role="alert">
      
      <div class="flex-grow-1">
        ‚ö†Ô∏è Your account is not verified. <br class="d-md-none" />
        <%= resendAttempts >= 3
          ? "(You‚Äôve reached the resend limit for today)"
          : "Please verify your email to unlock full features." %>
      </div>

      <div>
        <% if (resendAttempts < 3) { %>
          <form action="/resend-verification" method="POST" class="d-inline">
            <input type="hidden" name="email" value="<%= email %>">
            <button type="submit" class="btn btn-sm btn-outline-primary">Resend Email</button>
          </form>
        <% } else { %>
          <button class="btn btn-sm btn-outline-secondary" disabled>Resend Disabled</button>
        <% } %>
      </div>
      
    </div>
    </div>
    </div>
    <% } %>
<!-- Navbar -->
      <%- include('../partials/navbar_carts', {page:page}) %>
<!-- user dashboard section -->
      <section class="user-dashboard">
  <div class="container py-2">
    <div class="row">
      <!-- Left Column: User Profile Summary -->
      <div class="col-lg-4">
        <!-- Profile Card -->
        <div class="card mb-4">
          <div class="card-body text-center">
            <!-- Profile Image -->
            <img
              src="/users_images/<%= loggedInUser.img %>"
              alt="avatar"
              class="rounded-circle img-fluid"
              style="width: 150px; height: 150px"
            />
            <!-- Username -->
            <h5 class="my-3">@<%= loggedInUser.firstname %></h5>
            <!-- Date Joined -->
            <p class="text-muted mb-1">
              Date joined: <%= new Date(loggedInUser.date_joined).toLocaleDateString() %>
            </p>
            <!-- Location -->
            <p class="text-muted mb-4">
              <%= loggedInUser.city %> <%= loggedInUser.province %>
            </p>
            <!-- Edit/Delete Account Buttons -->
            <div class="d-flex justify-content-center mb-2">
              <!-- Edit Button -->
              <a href="/edit-profile">
                <button
                  type="button"
                  class="btn btn-dark"
                  data-mdb-button-init
                  data-mdb-ripple-init
                >
                  Edit ACCOUNT
                </button>
              </a>
              <!-- Hidden Form for DELETE request -->
              <form
                id="delete-profile-form"
                action="/user-profile"
                method="post"
                style="display: none"
              >
                <input type="hidden" name="_method" value="DELETE" />
              </form>
              <!-- Delete Button (Triggers Modal) -->
              <button
                type="button"
                class="btn btn-danger ms-1"
                data-mdb-button-init
                data-mdb-ripple-init
                data-bs-toggle="modal"
                data-bs-target="#staticBackdrop"
                aria-controls="staticBackdrop"
              >
                DELETE ACCOUNT
              </button>
            </div>
          </div>
        </div>

        <!-- Quick Links Card -->
        <div class="card mb-4 mb-lg-0">
          <div class="card-body p-0">
            <ul class="list-group list-group-flush rounded-3">
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class="fas fa-solid fa-envelope fa-beat fa-lg text-warning"></i>
                <p class="mb-0"><a href="/email">/mail</a></p>
              </li>
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class="fas fa-solid fa-bell fa-lg fa-beat text-danger"></i>
                <p class="mb-0"><a href="/inbox">/notifications</a></p>
              </li>
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class="fas fa-solid fa-inbox fa-beat fa-lg text-info"></i>
                <p class="mb-0"><a href="/orders">/orders</a></p>
              </li>
              <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                <i class="fas fa-thin fa-message fa-beat fa-lg" style="color: #667BC6;"></i>
                <p class="mb-0"><a href="/review">/reviews</a></p>
              </li>
            </ul>
          </div>
        </div>
      </div>

      <!-- Right Column: Detailed Info and Stats -->
      <div class="col-lg-8">
        <!-- Personal Info Card -->
        <div class="card mb-4">
          <div class="card-body">
            <div class="row mb-3">
              <div class="col-sm-4 fw-bold">Full Name</div>
              <div class="col-sm-8"><%= loggedInUser.firstname %> <%= loggedInUser.lastname %></div>
            </div>
            <div class="row mb-3">
              <div class="col-sm-4 fw-bold">Email</div>
              <div class="col-sm-8 text-break"><%= loggedInUser.email %></div>
            </div>
            <div class="row mb-3">
              <div class="col-sm-4 fw-bold">Phone</div>
              <div class="col-sm-8"><%= loggedInUser.phone %></div>
            </div>
            <div class="row">
              <div class="col-sm-4 fw-bold">Address</div>
              <div class="col-sm-8 text-break">
                <%= loggedInUser.address %> <%= loggedInUser.city %> <%= loggedInUser.province %>
              </div>
            </div>
          </div>
        </div>

        <!-- Stats Card -->
        <div class="row">
          <div class="col-12">
            <div class="card mb-4 mb-md-0" style="height: auto;">
              <div class="card-body">
                <!-- Stats Header -->
                <h4 class="mb-4 text-dark font-italic">üìä Your Stats Board</h4>

                <!-- Stat Items -->
                <div class="card shadow-sm border-0">
                  <div class="card-body">
                    <ul class="list-group list-group-flush">
                      <li class="list-group-item d-flex justify-content-between align-items-center">
                        üõí Orders Placed
                        <span class="badge bg-success rounded-pill fs-6"><%= stats.orders %></span>
                      </li>
                      <li class="list-group-item d-flex justify-content-between align-items-center">
                        ‚úçÔ∏è Reviews Given
                        <span class="badge bg-info rounded-pill fs-6"><%= stats.reviews %></span>
                      </li>
                      <li class="list-group-item d-flex justify-content-between align-items-center">
                        üíñ Wishlist Items
                        <span class="badge bg-warning text-dark rounded-pill fs-6"><%= stats.wishlist %></span>
                      </li>
                      <li class="list-group-item d-flex justify-content-between align-items-center">
                        üîî Notifications
                        <span class="badge bg-danger rounded-pill fs-6"><%= stats.notifications %></span>
                      </li>
                    </ul>
                  </div>
                </div>

                <!-- Motivational Footer -->
                <div class="text-center mt-3">
                  <small class="text-muted">Keep going! Your journey's just getting started üöÄ</small>
                </div>
              </div>
            </div>
          </div>
        </div> <!-- End Stats Row -->
      </div> <!-- End Right Column -->
    </div> <!-- End Row -->
  </div> <!-- End Container -->
        </section>
<!-- Alert for account deletion -->
      <div
        class="modal fade"
        id="staticBackdrop"
        data-bs-backdrop="static"
        data-bs-keyboard="false"
        tabindex="-1"
        aria-labelledby="staticBackdropLabel"
        aria-hidden="true"
      >
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header bg-danger text-white">
              <h5 class="modal-title" id="confirmDeleteModalLabel">
                Confirm Deletion
              </h5>
              <button
                type="button"
                class="btn-close"
                data-bs-dismiss="modal"
                aria-label="Close"
              ></button>
            </div>
            <div class="modal-body">
              <p>Are you sure you want to delete your account?</p>
            </div>
            <div class="modal-footer">
              <button
                type="button"
                class="btn btn-secondary"
                data-bs-dismiss="modal"
              >
                Cancel
              </button>
              <button
                type="button"
                class="btn btn-danger"
                onclick="submitDeleteProfileForm()"
              >
                Delete
              </button>
            </div>
          </div>
        </div>
      </div>
