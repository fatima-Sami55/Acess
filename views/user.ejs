    <script>
      // Replace image source if it contains '/public'
      const imgElement = document.querySelector(".img-responsive");
      if (imgElement) {
        let currentSrc = imgElement.src;
        let newSrc = currentSrc.replace("/public", "");
        imgElement.src = newSrc;
      }

      // Get the modal element
      const modal = document.getElementById("confirmDeleteModal");

      // Function to submit the delete profile form
      function submitDeleteProfileForm() {
        document.getElementById("delete-profile-form").submit();
      }
    </script>


