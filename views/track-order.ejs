            class="w-full divide-y divide-gray-200 overflow-hidden rounded-lg border border-gray-200 dark:divide-gray-700 dark:border-gray-700 lg:max-w-xl xl:max-w-2xl min-h-[400px]"
            style="margin-bottom: 90px"
          >
            <% orders.forEach((order, index) =>{ %> <% if (order.purchase_id ===
            verified_id) { %>
            <div class="space-y-4 p-6">
              <div class="flex items-center gap-6">
                <a href="#" class="h-14 w-14 shrink-0">
                  <img
                    class="h-full w-full dark:hidden"
                    src="<%= order.item_img%>"
                    alt="imac image"
                  />
                </a>

                <p><%= order.itemDescription%></p>
              </div>

              <div class="flex items-center justify-between gap-4">
